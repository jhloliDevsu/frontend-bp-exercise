{
  "collectCoverage": true,
  "collectCoverageFrom": [
    "src/**/*.{ts,tsx}"
  ],
  "coveragePathIgnorePatterns": [
    "node_modules",
    "test-config",
    "<rootDir>/src/index.tsx",
    "<rootDir>/src/App.tsx"
  ],
  "coverageDirectory": "<rootDir>/coverage/",
  "transformIgnorePatterns": [
    "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|cjs|ts|tsx)$"
  ],
  "verbose": false,
  "testEnvironment": "jsdom",
  "moduleDirectories": [
    "node_modules",
    "src"
  ]
}